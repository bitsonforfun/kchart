<view class="mine-wrapper">
  <view class="avatar-wrapper">
    <view>
      <view class="avatar">
        <image style="border-radius:50%;" src="{{userinfo.avatarUrl ? userinfo.avatarUrl:'../../static/img/home.png'}}"></image>
      </view>
      <view class="text">
        <!-- <text wx:if="{{userinfo.nickName}}">{{userinfo.nickName}}</text>
        <text wx:else bindtap="toLogin">注册 / 登录</text> -->
        <text wx:if="{{userinfo.nickName}}">{{userinfo.nickName}}</text>
        <button wx:if="{{ !userinfo.nickName }}" open-type="getUserInfo" bindgetuserinfo="toLogin">登录</button>
      </view>
    </view>
  </view>

  <zan-panel class="cell-panel-demo" title="{{ }}">
    <zan-cell-group>
      <zan-cell bindtap="openCurrencyUnitsheet" title="{{ labelCurrencyUnit }}" value="{{ currencyUnit }}" is-link></zan-cell>
      <zan-cell bindtap="onAboutUs" title="{{ aboutUs }}" value="{{ }}" is-link></zan-cell>
    </zan-cell-group>
  </zan-panel>
  <zan-panel title="">
    <zan-row>
      <zan-col col="2" col-class="custom-zan-col"> </zan-col>
      <zan-col col="20" col-class="custom-zan-col">ID: {{ userId }}</zan-col>
      <zan-col col="2" col-class="custom-zan-col"> </zan-col>
    </zan-row>
  </zan-panel>

  <zan-actionsheet
    show="{{ show }}"
    actions="{{ actions }}"
    cancel-text="{{  }}"
    cancel-with-mask="{{ cancelWithMask }}"
    bind:cancel="closeCurrencyUnitsheet"
    bind:actionclick="clickSelectCurrencyUnit"
    mask-class="tiny"
  />
  <!-- <view class="list-wrapper">
    <view class="weui-cells weui-cells_after-title">
      <block wx:for="{{menuitems}}" wx:key="menu_for">
        <navigator url="{{item.url}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__hd">
            <image src="{{item.icon}}"></image>
          </view>
          <view class="weui-cell__bd">{{item.text}}</view>
          <view class="weui-cell__ft weui-cell__ft_in-access">{{item.tips}}</view>
        </navigator>
      </block>
    </view>
  </view> -->
</view>