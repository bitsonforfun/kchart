<zan-panel class="panel-example">
  <view style="padding-top:30rpx;padding-left:30rpx;padding-bottom:0;font-size:70rpx">$ {{ currencyInfo.quotesUSDPrice }}</view>
  <view style="padding-top:4rpx;padding-left:30rpx;padding-bottom:0rpx;font-size:32rpx;color:{{ changePercentColor }}">{{ currencyInfo.quotesUSDPercentChange24h }}% ({{ label24Hour }})</view>
  <view style="padding-top:4rpx;padding-left:30rpx;padding-bottom:6rpx;font-size:26rpx;color:#A0A0A0">{{ labelVolume }} {{ currencyInfo.quotesUSDVolume24h }} USD</view>
</zan-panel>
 
<!-- wx chart -->
<view class="container3" style=''>
    <canvas canvas-id="areaCanvas" class="canvas" bindtouchstart="touchHandler"></canvas>
</view>

<block wx:if="{{coinpairs}}">
  <zan-panel class="cell-panel-demo" title="{{ labelCoinPair }}">
    <zan-cell-group>
      <block wx:for="{{coinpairs}}" wx:for-item="pair" wx:key="cell-{{pair.symbol}}">
        <zan-cell bindtap="open" data-pair="{{ pair }}" title="{{ pair.ex }}, {{ pair.symbol }}" value="{{ }}" is-link></zan-cell>
      </block>
    </zan-cell-group>
  </zan-panel>
</block>


<zan-panel title="{{ labelStats }}" class="panel-example">
  <zan-cell-group>
    <zan-cell title="{{ labelRank }}" label="" value="{{ currencyInfo.rank }}"></zan-cell>
    <zan-cell title="{{ labelMarketCap }}" label="" value="{{ currencyInfo.quotesUSDMarketCap }}"></zan-cell>
    <zan-cell title="{{ labelCirculatingSupply }}" label="" value="{{ currencyInfo.circulatingSupply }}"></zan-cell>
    <zan-cell title="{{ labelTotalSupply }}" label="" value="{{ currencyInfo.totalSupply }}"></zan-cell>
    <zan-cell title="{{ labelMaxSupply }}" label="" value="{{ currencyInfo.maxSupply }}"></zan-cell>
  </zan-cell-group>
</zan-panel>